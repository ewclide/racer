{
    "compilerOptions": {
        "outDir": "./dist/",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "allowJs": false,
        "downlevelIteration": true,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules/**/*.ts"
    ]
}